<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.uci.ics.smartband">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".index"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".carbon"
            android:label="@string/title_activity_carbon" />
        <activity
            android:name=".temperature"
            android:label="@string/title_activity_temperature" />
        <activity
            android:name=".heartbeat"
            android:label="@string/title_activity_heartbeat" />
        <activity
            android:name=".step"
            android:label="@string/title_activity_step" />
        <activity
            android:name=".UV"
            android:label="@string/title_activity_uv"></activity>
    </application>

</manifest>